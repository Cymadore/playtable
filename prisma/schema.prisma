generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model gamelist {
  id        Int      @id @default(autoincrement())
  name  String @unique
  img String
  count String
  text String
  rent String[]
  point String
  createdAt      DateTime @default(now())
}